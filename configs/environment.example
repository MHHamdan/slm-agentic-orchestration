# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/slm_orchestration
REDIS_URL=redis://localhost:6379/0

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# GitHub Token (with workflow scope for Actions)
GITHUB_TOKEN=your_github_token_with_workflow_scope

# Model Configuration
DEFAULT_SLM_MODEL=microsoft/DialoGPT-medium
DEFAULT_LLM_MODEL=gpt-3.5-turbo

# Security
SECRET_KEY=your_secret_key_for_jwt_tokens

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
ENVIRONMENT=development